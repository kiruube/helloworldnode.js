doctype html
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Document
  link(rel='stylesheet' href='https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css' integrity='sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm' crossorigin='anonymous')
.container
  h1 FORM
  br
  form(action='contact' method='POST')
    .form-row
      .form-group.col-md-6
        label(for='inputEmail4') Email
        input#inputEmail4.form-control(type='email' placeholder='Email' name='email')
      .form-group.col-md-6
        label(for='inputPassword4') Password
        input#inputPassword4.form-control(type='password' placeholder='Password' name='password')
    .form-group
      label(for='inputAddress') Address
      input#inputAddress.form-control(type='text' placeholder='1234 Main St' name='address')
    .form-group
      label(for='inputAddress2') Appartment Number
      input#inputAddress2.form-control(type='number' placeholder='Apartment, studio, or floor' name='appartment')
    .form-row
      .form-group.col-md-6
        label(for='inputCity') City
        input#inputCity.form-control(type='text' name='city')
      .form-group.col-md-4
        label(for='inputState') State
        select#inputState.form-control(name='state')
          option(selected='') Choose state...
          option Lira
          option Gulu
          option Arua
      .form-group.col-md-2
        label(for='inputZip') Zip
        input#inputZip.form-control(type='text' name='zip')
    .form-group
      .form-check
        input#gridCheck.form-check-input(type='checkbox' name='chechOut')
        label.form-check-label(for='gridCheck')
          | Check me out
    button.btn.btn-primary(type='submit') Sign in
script(src='https://code.jquery.com/jquery-3.2.1.slim.min.js' integrity='sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN' crossorigin='anonymous')
script(src='https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js' integrity='sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q' crossorigin='anonymous')
script(src='https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js' integrity='sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl' crossorigin='anonymous')
